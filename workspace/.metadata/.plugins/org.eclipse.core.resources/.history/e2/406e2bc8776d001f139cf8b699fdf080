
table inpr 16 8 {1000,707,0,-707,-1000,-707,0,707}

operation reorder {out inf a, in inpr b}{}
{
	wire [15:0]  a0=b[0];
	wire [15:0]  a1=b[4];
	wire [15:0]  a2=b[2];
	wire [15:0]  a3=b[6];
	wire [15:0]  a4=b[1];
	wire [15:0]  a5=b[5];
	wire [15:0]  a6=b[3];
	wire [15:0]  a7=b[7];
			
}

/*
 *
 *MAC
 *
*/


operation FFT_MAC {out AR tr,out AR ti, in AR a, in AR b, in AR c, in AR d}{}
{
	wire [31:0] r1=TIEmul(a[15:0],b[15:0],1'b1)>>15;
	wire [31:0] r2=TIEmul(c[15:0],d[15:0],1'b1)>>15;
	wire [31:0] r3=TIEmul(a[15:0],d[15:0],1'b1)>>15;
	wire [31:0] r4=TIEmul(c[15:0],b[15:0],1'b1)>>15;
	assign tr=r1-r2;
	assign ti=r3+r4;

}


/*
 *
 *SIMD
 *
*/

operation FFT_SIMD{}{}
{
		
}
